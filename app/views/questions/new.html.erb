<h1>Create a new Question page</h1>
    <%#= form_for(@question, :url => category_questions_path(@category)) do |f| %>
    <%= form_for([@category, Question.new]) do |f| %>
    <%= f.label :title %><br>
    <%= f.text_field :title , :class => "form-control-plaintext"%><br>
    <%= f.label :content %><br>
    <%= f.text_area :content , :class => "form-control-plaintext"%><br>

    <%= f.submit :class => "btn btn-primary" %>
    <% end %>